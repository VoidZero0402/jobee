<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TextField">
        <attr name="inputType" format="enum">
            <enum name="text" value="0" />
            <enum name="password" value="1" />
            <enum name="phone" value="2" />
            <enum name="email" value="3" />
        </attr>
        <attr name="hint" format="string" />
        <attr name="text" format="string" />
        <attr name="icon" format="reference" />
        <attr name="action" format="enum">
            <enum name="none" value="0" />
            <enum name="password" value="1" />
            <enum name="clear" value="2" />
            <enum name="custom" value="3" />
        </attr>
        <attr name="actionIcon" format="reference" />
        <attr name="animationDuration" format="integer" />
    </declare-styleable>
    <declare-styleable name="Chip">
        <attr name="checked" format="boolean" />
        <attr name="checkable" format="boolean" />
        <attr name="animationDuration" />
    </declare-styleable>
    <declare-styleable name="ChipGroup">
        <attr name="wrapMode" format="enum">
            <enum name="none" value="0" />
            <enum name="chain" value="1" />
            <enum name="aligned" value="2" />
        </attr>
        <attr name="horizontalGap" format="dimension" />
        <attr name="verticalGap" format="dimension" />
        <attr name="singleSelection" format="boolean" />
    </declare-styleable>
    <declare-styleable name="Switch">
        <attr name="checked" />
        <attr name="animationDuration" />
    </declare-styleable>
    <declare-styleable name="ListItem">
        <attr name="text" />
        <attr name="icon" />
        <attr name="tintColor" format="color" />
        <attr name="navigationArrowVisible" format="boolean" />
    </declare-styleable>
    <declare-styleable name="TabLayout">
        <attr name="scrollable" format="boolean" />
    </declare-styleable>
    <declare-styleable name="NavItem">
        <attr name="text" />
        <attr name="icon" />
        <attr name="activeIcon" format="reference" />
    </declare-styleable>
</resources>